{
  "name": "atlas",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "react-scripts": "2.1.5"
  },
  "dependencies": {
    "antd": "^3.19.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.19.0",
    "dotenv": "^6.2.0",
    "dropbox": "^4.0.16",
    "express": "^4.16.4",
    "googleapis": "^37.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "qs": "^6.6.0",
    "ramda": "^0.26.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "sequelize": "^5.8.9",
    "sequelize-cli": "^5.5.0"
  },
  "scripts": {
    "start": "./node_modules/concurrently/bin/concurrently.js \"npm run client\" \"npm run server\"",
    "server": "./node_modules/nodemon/bin/nodemon.js server/server.js",
    "db:setup": "cd server && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
